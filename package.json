{
    "name": "kakidash",
    "version": "0.0.1",
    "description": "Mindmap Javascript Library",
    "type": "module",
    "main": "./dist/kakidash.umd.js",
    "module": "./dist/kakidash.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/kakidash.es.js",
            "require": "./dist/kakidash.umd.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite --host",
        "build": "tsc && vite build",
        "test": "vitest",
        "lint": "eslint src tests --ext .ts",
        "format": "prettier --write \"src/**/*.{ts,json}\""
    },
    "keywords": [
        "mindmap",
        "javascript",
        "typescript"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@types/argparse": "^2.0.17",
        "@types/estree": "^1.0.8",
        "@types/node": "^25.0.5",
        "@types/whatwg-mimetype": "^3.0.2",
        "@types/ws": "^8.18.1",
        "@typescript-eslint/eslint-plugin": "^7.0.0",
        "@typescript-eslint/parser": "^7.0.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "happy-dom": "^20.0.11",
        "prettier": "^3.2.5",
        "typescript": "^5.4.0",
        "vite": "^5.2.0",
        "vite-plugin-dts": "^3.8.0",
        "vitest": "^1.4.0"
    }
}
