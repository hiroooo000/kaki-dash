{
    "name": "kakidashi-board",
    "version": "0.0.1",
    "description": "Mindmap Javascript Library",
    "type": "module",
    "main": "./dist/kakidashi-board.umd.js",
    "module": "./dist/kakidashi-board.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/kakidashi-board.es.js",
            "require": "./dist/kakidashi-board.umd.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "test": "vitest",
        "lint": "eslint src tests --ext .ts",
        "format": "prettier --write \"src/**/*.{ts,json}\""
    },
    "keywords": [
        "mindmap",
        "javascript",
        "typescript"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^7.0.0",
        "@typescript-eslint/parser": "^7.0.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "prettier": "^3.2.5",
        "typescript": "^5.4.0",
        "vite": "^5.2.0",
        "vite-plugin-dts": "^3.8.0",
        "vitest": "^1.4.0"
    }
}